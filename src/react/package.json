{
    "name": "memento.react",
    "version": "1.0.4",
    "description": "memento-js binding for react. Flexible and easy unidirectional store library for state management with Dependency Injection for React or other frontend apps.",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "repository": "https://github.com/le-nn/memento",
    "author": "le-nn",
    "license": "MIT",
    "private": false,
    "keywords": [
        "react",
        "memento",
        "react",
        "store",
        "store-pattern",
        "state-management"
    ],
    "scripts": {
        "build": "yarn rollup -c rollup.config.js",
        "start": "yarn rollup -c rollup.config.js --watch"
    },
    "dependencies": {
        "memento.core": "link:../core"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-transform-runtime": "^7.18.2",
        "@rollup/plugin-babel": "^5.0.2",
        "@rollup/plugin-commonjs": "^22.0.0",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@rollup/plugin-typescript": "^8.1.0",
        "@types/react": "^18.0.11",
        "rollup": "^2.36.0",
        "rollup-plugin-typescript2": "^0.31.2",
        "typescript": "^4.1.3"
    },
    "peerDependencies": {
        "react": "^16.8.3 || ^17 || ^18"
    }
}
